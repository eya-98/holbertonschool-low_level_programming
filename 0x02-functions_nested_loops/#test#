#include "holberton.h"
/**
 *print_to_98 -  prints all natural numbers from n to 98
 *@n : integer
 * Return: Always 0.
 */
void print_to_98(int n)
{
int i, a, b, j;
a = n % 10;
b = n / 10;
i = b;
j = a;
if (b > 9)
for (i = 'b'; i >= '9'; i--)
for (j = 'a'; j >= '8'; j--)
while ((b =! 9) && (a != 8))
{
_putchar('0' + i);
_putchar('0' + j);
_putchar(',');
_putchar(' ';
}



if (b < 9) 
i++
  

